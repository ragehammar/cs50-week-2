#include <cs50.h>
#include <stdio.h>


int main(void)
{

    int arr[6] = {1};

    printf("value is %i\n", arr[0]);
    
    for (int i=1 ; i<6 ; i++)
    {
        arr[i] = arr[i-1] * 2;
        printf("Value is %i\n", arr[i]);
    }

       // printf("%i %i %i %i %i %i \n" , arr[0], arr[1], arr[2], arr[3], arr[4], arr[5]);


}
